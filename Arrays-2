package com.demo.arrays;

public class ArraysDemo {

	public static void main(String[] args) {

		String names[] = new String[3];
		names[0] = "Raj";
		names[1] = "Kumar";
		names[2] = "Ravi";

		for (int i = 0; i <= names.length; i++) {
			System.out.println(names[i]);
		}

	}

}


package com.demo.arrays;

public class ArraysDemo {

	public static void main(String[] args) {

		String names[] = new String[3];
		names[0] = "Raj";
		names[1] = "Kumar";
		names[2] = "Ravi";

		for (int i = names.length - 1; i >= 0; i--) {
			System.out.println(names[i]);
		}

	}

}


